---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:atom="http://www.w3.org/2005/Atom" version="2.0" xmlns:posterous="http://posterous.com/help/rss/1.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:media="http://search.yahoo.com/mrss/">
  <channel>
    <title>Michiel Overeem's blog</title>
    <description>Michiel Overeem's blog</description>
    <generator>Jekyll</generator>
    <link href="http://blog.movereem.nl/rss.xml" rel="self"/>
    <link href="http://blog.movereem.nl/"/>
    <atom:link href="http://blog.movereem.nl/rss.xml" rel="self"/>
    
    {% for post in site.posts limit:50 %}
    <item>
	    <title>{{ post.title | xml_escape }}</title>
	    <link href="http://blog.movereem.nl{{ post.url }}"/>
        <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
 	    <guid>http://blog.movereem.nl{{ post.id }}</guid>
		<description type="html">{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}
  </channel>
</rss>
